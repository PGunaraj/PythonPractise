n = int(input())
s = set(map(int, input().split())) 
N= int(input())
i=1
while i<=N:
    x=input().split()
    if x[0] =='pop':
        s.pop()
    elif x[0] == 'remove':
        s.remove(int(x[1]))
    elif x[0] == 'discard':
        s.discard(int(x[1]))
    else:
        print("Error: Enter a valid operation to do")
    i += 1
print(sum(s))
